/**
 * Wrapper with a copy connection by renaming the net interface
 */

L = box L( out l1, out l2 )
R = box R( in r1, in r2 )
wrapper W( in l1(a1, a2), in l2(a1, a2), out r1(b1, b2), out r2(b1, b2) ) {
    A1 = box A ( in a1, out p_int )
    A2 = box A ( in a2, out p_int )
    B = box B ( up in p_int, down out p_int )
    C1 = box C ( in p_int, out b1 )
    C2 = box C ( in p_int, out b2 )
    connect (A1|A2).B.(C1|C2)
} net ( up in a1(l1, l2), up in a2(l1, l2), down out b1(r1, r2), down out b2(r1, r2) )

connect L.W.R
