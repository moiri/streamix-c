/**
 * Hierarchical wrapper structure with parallel connections, with port renaming
 * on the wrapper interfaces
 */

A = box A( out a )

wrapper W1( in a(a_int) ) {
    A1 = box A1( up in a_int, down out b )
    wrapper W2( in b(b_int) ) {
        A2 = box A2( in b_int )
        connect (A2|A2)
    } net( up in b_int )
    connect (A1|A1).W2
} net( up in a_int )

connect A.W1
