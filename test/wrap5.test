L = box L( out p1 )
R = box R( in p2 )
wrapper W { in p1{a1, a2}, out p2{b} } {
    link { A, this }
    link { B, this }
    A = box A ( in a1, in a2, out p_int )
    B = box B ( in p_int, out b )
    N = A.B
    net N{down in a1, down in a2, up out b}
    N1 = net N{down in a{a1, a2}, up out b}
}

L.W.R
