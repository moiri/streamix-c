/**
 * Wrapper with a feedback connection on the net
 */

L = box L( out p1 )
R = box R( in p2 )
wrapper W( in p1, out p2 ) {
    A = box A ( in a1, in a2, out p_int )
    B = box B ( up in p_int, down out p_int )
    C = box C ( in p_int, out b1, out b2 )
    connect A.B.C
} net ( up in a1(p1), up in a2(r), down out b1(p2), down out b2(r) )

connect L.W.R
